                                                <td><a href="/ModifyExperiment:{{experiment_id}}">{{experiment_name}}</a></td>
                                                <td>{{last_modified}}</td>
                                                <td>{{experiment_desc}}</td>
                                                <td>{{active}}</td>
                                                <td>
                                                    <a href="/DeleteExperiment:{{experiment_id}}" class="del"><i class="fa fa-fw fa-trash-o"></i></a>
                                                </td>
                                              </tr>
                                          {{/each}} 
